{% block stylesheets %}
{% endblock %}

{% block content %}
    {% if ads != false %}
        {{ ads.code|raw }}
        {% if ads.getDownloadLink %}
            <img class="lsoft-ad-image" src="{{ ads.getDownloadLink }}" width="100%" height="100%">
        {% endif %}
    {% elseif ads == false and is_granted('ROLE_SUPER_ADMIN', app.user) %}
        <p>Ad by {{ zone }} not found </p>
    {% endif %}
{% endblock %}
